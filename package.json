{
  "name": "@gitborlando/geo",
  "version": "5.2.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "re-i": "rm -rf node_modules pnpm-lock.yaml",
