  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.0.15",
    "@vitest/ui": "^3.2.4",
    "pretty-quick": "^4.2.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "simple-git-hooks": "^2.13.1"
  },
  "prettier": {
    "printWidth": 85,
    "jsxBracketSameLine": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "simple-git-hooks": {
